{
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "vercel_app.py",
        "VERCEL_ENV": "production",
        "PYTHONPATH": ".",
        "PYTHON_VERSION": "3.9",
        "MAX_CONTENT_LENGTH": "16777216"
    }
} 